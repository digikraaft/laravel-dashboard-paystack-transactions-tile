{
    "name": "digikraaft/laravel-dashboard-paystack-transactions-tile",
    "description": "A tile to show list of Paystack Transactions on Laravel Dashboard",
    "keywords": [
        "digikraaft",
        "laravel-dashboard",
        "paystack-dashboard",
        "paystack"
    ],
    "homepage": "https://github.com/digikraaft/laravel-dashboard-paystack-transactions-tile",
    "license": "MIT",
    "authors": [
        {
            "name": "Tim Oladoyinbo",
            "email": "dev@digitalkraaft.com",
            "homepage": "https://digitalkraaft.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "digikraaft/paystack-php": "^1.0",
        "illuminate/console": "^7.24",
        "illuminate/pagination": "^7.24",
        "illuminate/support": "^7.24",
        "spatie/laravel-dashboard": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Digikraaft\\PaystackTransactionsTile\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Digikraaft\\PaystackTransactionsTile\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Digikraaft\\PaystackTransactionsTile\\PaystackTransactionsTileServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
